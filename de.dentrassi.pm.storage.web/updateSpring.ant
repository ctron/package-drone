<?xml version="1.0" encoding="UTF-8"?>
<!-- ======================================================================
     Fetch spring
     ====================================================================== -->
<project name="project" default="default">
    <description>
    	Fetch spring
    </description>

	<property name="spring.version" value="4.1.2.RELEASE"/>
	
    <!-- ================================= 
          target: default
         ================================= -->
    <target name="default">
    	<get src="http://repo1.maven.org/maven2/org/springframework/spring-context/${spring.version}/spring-context-${spring.version}.jar" dest="WEB-INF/lib" usetimestamp="true"/>
		<get src="http://repo1.maven.org/maven2/org/springframework/spring-beans/${spring.version}/spring-beans-${spring.version}.jar" dest="WEB-INF/lib" usetimestamp="true"/>
    	<get src="http://repo1.maven.org/maven2/org/springframework/spring-core/${spring.version}/spring-core-${spring.version}.jar" dest="WEB-INF/lib" usetimestamp="true"/>
    	<get src="http://repo1.maven.org/maven2/org/springframework/spring-expression/${spring.version}/spring-expression-${spring.version}.jar" dest="WEB-INF/lib" usetimestamp="true"/>
    	<get src="http://repo1.maven.org/maven2/org/springframework/spring-aop/${spring.version}/spring-aop-${spring.version}.jar" dest="WEB-INF/lib" usetimestamp="true"/>
    	<get src="http://repo1.maven.org/maven2/org/springframework/spring-web/${spring.version}/spring-web-${spring.version}.jar" dest="WEB-INF/lib" usetimestamp="true"/>
    	<get src="http://repo1.maven.org/maven2/org/springframework/spring-webmvc/${spring.version}/spring-webmvc-${spring.version}.jar" dest="WEB-INF/lib" usetimestamp="true"/>
    	<get src="http://repo1.maven.org/maven2/org/springframework/spring-webmvc/${spring.version}/spring-webmvc-${spring.version}-sources.jar" dest="WEB-INF/lib" usetimestamp="true"/>
    	
    	<get src="http://repo1.maven.org/maven2/org/hibernate/hibernate-validator/5.1.3.Final/hibernate-validator-5.1.3.Final.jar" dest="WEB-INF/lib" usetimestamp="true"/>
    	<get src="http://repo1.maven.org/maven2/org/hibernate/hibernate-validator-annotation-processor/5.1.3.Final/hibernate-validator-annotation-processor-5.1.3.Final.jar" dest="WEB-INF/lib" usetimestamp="true"/>
    	<get src="http://repo1.maven.org/maven2/javax/validation/validation-api/1.1.0.Final/validation-api-1.1.0.Final.jar" dest="WEB-INF/lib" usetimestamp="true"/>
    	
    </target>

</project>
